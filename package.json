{
  "name": "crimson-haze",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.1.1",
    "@types/node": "^17.0.25",
    "@types/react": "^18.0.6",
    "@types/react-dom": "^18.0.2",
    "classnames": "^2.3.2",
    "eslint-config-airbnb-typescript-prettier": "^5.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "formik": "^2.2.9",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-draggable": "^4.4.5",
    "react-redux": "^8.0.1",
    "screenfull": "^6.0.2",
    "typescript": "^4.6.0",
    "web-vitals": "^2.1.0",
    "yup": "^0.32.11"
  },
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "test": "jest",
    "test-coverage": "jest --collectCoverage",
    "lint": "eslint . --ext .ts,.tsx",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.{js,jsx,ts,tsx,css,md,json}' --write",
    "build-for-beta": "npm run build && rsync -r --stats ./dist/ talishar-beta:/opt/ReactClientRepo/build/"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/mocks/**"
    ],
    "setupFilesAfterEnv": [
      "./config/jest/setupTests.js"
    ],
    "testEnvironment": "jsdom",
    "modulePaths": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.(ts|js|tsx|jsx)$": [
        "jest-chain-transform",
        {
          "transformers": [
            "../../../config/jest/importMetaEnvTransformer.js",
            "@swc/jest"
          ]
        }
      ],
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!screenfull)",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "resetMocks": true
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-typescript": "^7.18.6",
    "@swc/core": "^1.3.22",
    "@swc/jest": "^0.2.24",
    "@types/jest": "^29.2.4",
    "@types/mocha": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "@vitejs/plugin-react": "^1.1.1",
    "babel-jest": "^29.0.3",
    "camelcase": "^6.3.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.0.3",
    "jest-chain-transform": "^0.0.5",
    "jest-environment-jsdom": "^29.3.1",
    "jest-watch-typeahead": "^2.2.1",
    "vite": "^2.9.15",
    "vite-plugin-svgr": "^2.2.2",
    "vite-tsconfig-paths": "^4.0.2"
  },
  "packageManager": "yarn@3.2.3",
  "homepage": ".",
  "volta": {
    "node": "17.9.1"
  }
}
